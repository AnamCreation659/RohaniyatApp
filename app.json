{
  "expo": {
    "name": "Idara Rohaniyat",
    "slug": "mobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/web-icon.png",
    "scheme": "idararohaniyat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/defultsplash.png",
      "backgroundColor": "#E4DAC1"
    },
    "ios": {
      "bundleIdentifier": "com.creationnext.idararohaniyat",
      "supportsTablet": true,
      "icon": "./assets/images/app-icon.png"
    },
    "android": {
      "package": "com.creationnext.idararohaniyat",
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "icon": "./assets/images/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
             "backgroundColor": "#E4DAC1"
      }
    },
    "web": {
      "favicon": "./assets/images/web-icon.png",
      "name": "Idara Rohaniyat",
      "themeColor": "#E4DAC1",
      "backgroundColor": "#E4DAC1",
      "display": "standalone"
    },
    "extra": {
      "eas": {
        "projectId": "fedaf04d-9035-4932-87db-2075d40cc42b"
      },
      "router": {}
    },
    "owner": "rohaniyatapp",
    "plugins": [
      "expo-router"
    ]
  }
}
