{
  "expo": {
    "name": "Rohaniyat",
    "slug": "mobileapp",
    "icon": "./assets/images/main-logo.png",
    "splash": {
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "adaptiveIcon": {
      "image": "./assets/images/balls.png"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      
      "package": "com.hamzawaqasch.rohaniyat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "410aabf1-0a0d-4667-9ef2-39b0bd270ba9"
      }
    }
  }
}
