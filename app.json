{
  "expo": {
    "name": "Rohaniyat",
    "slug": "mobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/MainBackground.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.hamzawaqasch.rohaniyat",
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon.png",
        "backgroundColor": "#E4DAC1"
      }
    },
    "web": {
      "favicon": "./assets/images/AppIcon.png"
    },
    "extra": {
      "eas": {
        "projectId": "410aabf1-0a0d-4667-9ef2-39b0bd270ba9"
      }
    }
  }
}
